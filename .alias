alias flutter='fvm flutter'
alias dart='fvm dart'
alias copy='clipboard'
alias flc='flutter clean'
alias flr='flutter run'
alias brw='flutter pub run build_runner watch --delete-conflicting-outputs --enable-experiment=super-parameters,enhanced-enums'
alias brb='flutter pub run build_runner build --delete-conflicting-outputs --enable-experiment=super-parameters,enhanced-enums'
alias brc='flutter pub run build_runner clean'
alias dfile='/usr/bin/git --git-dir=$HOME/.dotfiles/ --work-tree=$HOME'
alias cat='bat'
alias ncu='npx -p npm-check-updates ncu -u'
alias ls='lsd'
alias ping='gping'
alias clearpod='rm -rf ~/Library/Caches/CocoaPods && rm -rf Pods && rm -rf ~/Library/Developer/Xcode/DerivedData/* && pod deintegrate'
alias onlyMain='git branch | grep -v "main" | xargs git branch -D'
alias onlyStaging='git branch | grep -v "staging" | xargs git branch -D'
